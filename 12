score = 0
print ("welcome to easy quiz.")
name = input ("vad är Sveriges huvudstad?").lower()
if name == "Stockholm":
        print ("det stämmer")
        score +=1

else:
    print ("Det är fel")

print (f"Quizen är över, du fick {score} poäng.")

